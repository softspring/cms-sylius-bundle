{
    "name": "softspring/cms-sylius-bundle",
    "description": "Softspring CMS bundle Sylius integration",
    "type": "bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "softspring/cms-bundle": "^5.2",
        "sylius/theme-bundle": "^2.1.1 || ^2.3",
        "sylius/ui-bundle": "^1.11"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.59.*",
        "phpstan/phpstan": "^1.9",
        "rector/rector": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\CmsSyliusBundle\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
