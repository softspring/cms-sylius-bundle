{% trans_default_domain 'sfs_media_admin' %}
{% extends '@SfsMedia/admin_layout_semantic.html.twig' %}
{% block title %}{{ 'admin_medias.delete.meta.title'|trans }}{% endblock %}

{% block header_title %}{{ 'admin_medias.delete.meta.title'|trans }}{% endblock %}
{% block header_subtitle %}Manage content media{% endblock %}

{% block content %}
    <div class="ui hidden divider"></div>
    <div class="ui segment">
    {{ form_start(form,{'attr':{'class':'ui form'}}) }}
        {{ form_widget(form) }}
        {{ form_rest(form) }}
        <div class="ui hidden divider"></div>
        <div class="ui buttons">
            <input type="submit" value="{{ 'admin_medias.delete.actions.delete.button'|trans }}" class="ui labeled icon primary button" />
            <a href="{{ url('sfs_media_admin_medias_list') }}" class="ui button">{{ 'admin_medias.delete.actions.cancel.button'|trans }}</a>
        </div>
    {{ form_end(form) }}
    </div>
{% endblock content %}
